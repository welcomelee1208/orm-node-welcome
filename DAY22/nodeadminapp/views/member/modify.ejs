
            <!--우측 콘텐츠 헤더영역 -->
            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2>사용자 정보 수정</h2>
                    <ol class="breadcrumb">
                        <li>
                            <a href="/">Home></a>
                        </li>
                        <li>
                            <a>사용자 정보 수정></a>
                        </li>
                        
                    </ol>
                </div>
                <div class="col-lg-2">

                </div>
            </div>

            <!--우측 콘텐츠 영역 -->
            <div class="wrapper wrapper-content">
                    <form action="/member/modify/:mid" method="post">
                        <div class="ibox-content m-b-sm border-bottom">


                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">메일주소</label>
                                        <input type="text" name="email" class="form-control">
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">비밀번호</label>
                                        <input type="password" name="password"class="form-control">
                                    </div>
                                </div>

                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">성명</label>
                                        <input type="text" name="name"class="form-control">
                                    </div>
                                </div>

                            </div>

                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">프로필 사진 업로드</label>
                                        <input type="file"name="file" class="form-control">
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">전화번호</label>
                                        <input type="text" name="telephone"class="form-control">
                                    </div>
                                </div>

                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">생년월일</label>
                                        <input type="text" name="birthday"class="form-control">
                                    </div>
                                </div>

                            </div>

                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">회원가입 유형</label>
                                        <select name="entryTypeCode" class="form-control">
                                            <option value="0"> 직접가입</option>
                                            <option value="1">SNS-FACEBOOK</option>
                                            <option value="2">SNS-GOOGLE</option>
                                            <option value="3">SNS-KAKAO</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">가입상태 정보</label>
                                        <select name="entryStateCode" class="form-control">
                                            <option value="0">일시중지</option>
                                            <option value="1">사용중</option>
                                            <option value="2">탈퇴</option>
                                            
                                        </select>
                                    </div>
                                </div>

                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="control-label">가입 일시</label>
                                        <input type="text" class="form-control" value="2023-12-19" readonly>
                                        
                                    </div>
                                </div>

                            </div>
                            <div class="text-center">
                                <button type="submit" class="btn btn-primary">저장</button>
                                <a href="/member/list" class="btn btn-info">목록</a>
                                <button type="button" id="btnDelete" class="btn btn-danger"><a href="/member/list">삭제</a></button>
                            </div>

                        </div>
                    </form>
            </div>
